] + [ruggedness]\n","model = Word2Vec(sentences=docs, vector_size=200, window=4, hs=1, negative=0, min_count=1, sg=1)\n","\n","data = []\n","for keyword in train_list:\n","    columns = {'top20 키워드': keyword}\n","    for category, words in [('진실성', sincerity),\n","                            ('흥미로움', excitement),\n","                            ('유능함', competence),\n","                            ('세련됨', sophistication),\n","                            ('강인함', ruggedness)]:\n","        sim_sum = sum(model.wv.similarity(w1=keyword, w2=word) for word in words)\n","        columns[category] = sim_sum / len(words)\n","    data.append(columns)\n","\n","df_data = pd.DataFrame(data)\n","\n","df_data"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":677},"id":"kdA-I4ktg2O_","executionInfo":{"status":"ok","timestamp":1691018782738,"user_tz":-540,"elapsed":487,"user":{"displayName":"Yeseul Ryu","userId":"01398576250166115600"}},"outputId":"bbc3b29d-3a03-4d0d-b64d-a2772c575739"},"execution_count":180,"outputs":[{"output_type":"execute_result","data":{"text/plain":["   top20 키워드       진실성      흥미로움       유능함       세련됨       강인함\n","0         좋음  0.001900 -0.013746 -0.009551  0.071509  0.010428\n","1         다름 -0.001231 -0.059308 -0.027538 -0.042511  0.009345\n","2         빠름  0.009070 -0.005927 -0.004923  0.026828 -0.015535\n","3         예쁨 -0.001550 -0.017596 -0.061142  0.001626  0.008426\n","4        색다름  0.037708  0.017802 -0.001762 -0.005262 -0.010612\n","5         멋짐  0.025736  0.169255  0.066691 -0.018701  0.005121\n","6         짧음  0.012734 -0.021128  0.004604 -0.044569  0.007693\n","7        재밌음  0.012240 -0.032449  0.039676 -0.006490 -0.043479\n","8         어림  0.040607  0.003952  0.012788 -0.009961  0.004173\n","9        신선함  0.066300 -0.004322 -0.051531 -0.041149  0.044621\n","10      재미있음  0.000352  0.021723  0.026052 -0.050238 -0.043268\n","11        젊음 -0.010871  0.139805  0.061077 -0.004516  0.036988\n","12       화려함 -0.023177  0.078686  0.040